{
  "manifest_version": 3,
  "name": "React Chrome Extension Starter",
  "description": "React Chrome Extension Starter",
  "version": "1.0",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}